<form action="/travel_preferences" method="post">
  <div class="mb-3">
    <label for="destination" class="form-label">Destination</label>
    <input type="text" class="form-control" id="destination" name="travel_preference[destination]">
  </div>
  <div class="mb-3">
    <label for="start_date" class="form-label">Start Date</label>
    <input type="date" class="form-control" id="start_date" name="travel_preference[start_date]">
  </div>
  <div class="mb-3">
    <label for="end_date" class="form-label">End Date</label>
    <input type="date" class="form-control" id="end_date" name="travel_preference[end_date]">
  </div>
  <div class="mb-3">
    <label for="budget" class="form-label">Budget</label>
    <input type="number" class="form-control" id="budget" name="travel_preference[budget]">
  </div>
  <button type="submit" class="btn btn-primary">Create</button>
</form>
